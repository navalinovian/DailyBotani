<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundApp">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


      <androidx.constraintlayout.widget.ConstraintLayout
          android:layout_width="match_parent"
          android:layout_height="match_parent"
          >

          <ImageView
              android:id="@+id/back"
              android:layout_width="30dp"
              android:layout_height="30dp"
              android:src="@drawable/back"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="10dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="20dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:text="Kategori Tanaman"
              android:textColor="@color/tulisan"
              android:textSize="25dp"
              tools:ignore="MissingConstraints"
              app:layout_constraintLeft_toRightOf="@+id/back"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="3dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="70dp"
              android:id="@+id/kategori_tanaman"
              />

          <ImageView
              android:layout_width="30dp"
              android:layout_height="30dp"
              android:src="@drawable/search"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="365dp"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="10dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/search"
              />

          <ImageView
              android:layout_width="70dp"
              android:layout_height="70dp"
              android:src="@drawable/daun"
              android:id="@+id/daun_kategori"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="95dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="30dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:textSize="25dp"
              android:text="Tanaman Hias Daun"
              android:textColor="@color/tulisan"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="100dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="120dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/tanaman_hias_daun"/>

          <ImageView
              android:layout_width="70dp"
              android:layout_height="70dp"
              android:src="@drawable/bunga"
              android:id="@+id/bunga_kategori"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="195dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="30dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:textSize="25dp"
              android:text="Tanaman Hias Bunga"
              android:textColor="@color/tulisan"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="200dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="120dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/tanaman_hias_bunga"/>

          <ImageView
              android:layout_width="70dp"
              android:layout_height="70dp"
              android:src="@drawable/buah"
              android:id="@+id/buah_kategori"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="295dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="30dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:textSize="25dp"
              android:text="Tanaman Hias Buah"
              android:textColor="@color/tulisan"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="300dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="120dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/tanaman_hias_buah"/>

          <ImageView
              android:layout_width="70dp"
              android:layout_height="70dp"
              android:src="@drawable/batang"
              android:id="@+id/batang_kategori"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="395dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="30dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:textSize="25dp"
              android:text="Tanaman Hias Batang"
              android:textColor="@color/tulisan"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="400dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="120dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/tanaman_hias_batang"/>

          <ImageView
              android:layout_width="70dp"
              android:layout_height="70dp"
              android:src="@drawable/akar"
              android:id="@+id/akar_kategori"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="495dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="30dp"
              tools:ignore="MissingConstraints" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:fontFamily="@font/carter_one"
              android:textSize="25dp"
              android:text="Tanaman Hias Akar"
              android:textColor="@color/tulisan"
              app:layout_constraintTop_toTopOf="parent"
              android:layout_marginTop="500dp"
              app:layout_constraintLeft_toLeftOf="parent"
              android:layout_marginLeft="120dp"
              tools:ignore="MissingConstraints"
              android:id="@+id/tanaman_hias_akar"/>






      </androidx.constraintlayout.widget.ConstraintLayout>




    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>